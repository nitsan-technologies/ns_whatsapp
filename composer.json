{
    "name": "nitsan/ns-whatsapp",
    "type": "typo3-cms-extension",
    "description": "Ns Whatsapp",
    "authors": [],
    "require": {
        "typo3/cms-core": "^12"
    },
    "autoload": {
        "psr-4": {
            "Nitsan\\NsWhatsapp\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/ns-whatsapp": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor/",
        "bin-dir": ".Build/bin/"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_whatsapp"
        }
    }
}
